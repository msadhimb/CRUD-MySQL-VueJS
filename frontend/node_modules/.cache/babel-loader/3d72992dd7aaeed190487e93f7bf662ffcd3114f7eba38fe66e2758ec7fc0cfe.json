{"ast":null,"code":"export default {\n  name: \"InputGuest\",\n  data() {\n    return {\n      form: {\n        email: \"\",\n        name: \"\",\n        food: null,\n        checked: []\n      },\n      foods: [{\n        text: \"Select One\",\n        value: null\n      }, \"Carrots\", \"Beans\", \"Tomatoes\", \"Corn\"],\n      show: true\n    };\n  },\n  methods: {\n    onSubmit(event) {\n      event.preventDefault();\n      alert(JSON.stringify(this.form));\n    },\n    onReset(event) {\n      event.preventDefault();\n      // Reset our form values\n      this.form.email = \"\";\n      this.form.name = \"\";\n      this.form.food = null;\n      this.form.checked = [];\n      // Trick to reset/clear native browser form validation state\n      this.show = false;\n      this.$nextTick(() => {\n        this.show = true;\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AA2EA,eAAe;EACbA,IAAI,EAAE,YAAY;EAClBC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,KAAK,EAAE,EAAE;QACTH,IAAI,EAAE,EAAE;QACRI,IAAI,EAAE,IAAI;QACVC,OAAO,EAAE;MACX,CAAC;MACDC,KAAK,EAAE,CACL;QAAEC,IAAI,EAAE,YAAY;QAAEC,KAAK,EAAE;MAAK,CAAC,EACnC,SAAS,EACT,OAAO,EACP,UAAU,EACV,MAAM,CACP;MACDC,IAAI,EAAE;IACR,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQ,CAACC,KAAK,EAAE;MACdA,KAAK,CAACC,cAAc,EAAE;MACtBC,KAAK,CAACC,IAAI,CAACC,SAAS,CAAC,IAAI,CAACd,IAAI,CAAC,CAAC;IAClC,CAAC;IACDe,OAAO,CAACL,KAAK,EAAE;MACbA,KAAK,CAACC,cAAc,EAAE;MACtB;MACA,IAAI,CAACX,IAAI,CAACC,KAAI,GAAI,EAAE;MACpB,IAAI,CAACD,IAAI,CAACF,IAAG,GAAI,EAAE;MACnB,IAAI,CAACE,IAAI,CAACE,IAAG,GAAI,IAAI;MACrB,IAAI,CAACF,IAAI,CAACG,OAAM,GAAI,EAAE;MACtB;MACA,IAAI,CAACI,IAAG,GAAI,KAAK;MACjB,IAAI,CAACS,SAAS,CAAC,MAAM;QACnB,IAAI,CAACT,IAAG,GAAI,IAAI;MAClB,CAAC,CAAC;IACJ;EACF;AACF,CAAC","names":["name","data","form","email","food","checked","foods","text","value","show","methods","onSubmit","event","preventDefault","alert","JSON","stringify","onReset","$nextTick"],"sourceRoot":"","sources":["/Users/adhim/Documents/Working/VueJs/crudmysql/frontend/src/components/Input.vue"],"sourcesContent":["<template>\n  <main>\n    <div class=\"container pt-5\">\n      <h1 class=\"w-100\">Congratulation on your wedding</h1>\n      <div class=\"row\">\n        <div class=\"col-md-6\">\n          <img\n            src=\"../assets/undraw_wedding_re_66hj.png\"\n            alt=\"wedding\"\n            class=\"d-block img-fluid\"\n          />\n        </div>\n        <div class=\"col-md-6\">\n          <b-form @submit=\"onSubmit\" @reset=\"onReset\" v-if=\"show\">\n            <b-form-group\n              id=\"input-group-1\"\n              label=\"Email address:\"\n              label-for=\"input-1\"\n              description=\"We'll never share your email with anyone else.\"\n            >\n              <b-form-input\n                id=\"input-1\"\n                v-model=\"form.email\"\n                type=\"email\"\n                placeholder=\"Enter email\"\n                required\n              ></b-form-input>\n            </b-form-group>\n\n            <b-form-group\n              id=\"input-group-2\"\n              label=\"Your Name:\"\n              label-for=\"input-2\"\n            >\n              <b-form-input\n                id=\"input-2\"\n                v-model=\"form.name\"\n                placeholder=\"Enter name\"\n                required\n              ></b-form-input>\n            </b-form-group>\n\n            <b-form-group id=\"input-group-3\" label=\"Food:\" label-for=\"input-3\">\n              <b-form-select\n                id=\"input-3\"\n                v-model=\"form.food\"\n                :options=\"foods\"\n                required\n              ></b-form-select>\n            </b-form-group>\n\n            <b-form-group id=\"input-group-4\" v-slot=\"{ ariaDescribedby }\">\n              <b-form-checkbox-group\n                v-model=\"form.checked\"\n                id=\"checkboxes-4\"\n                :aria-describedby=\"ariaDescribedby\"\n              >\n                <b-form-checkbox value=\"me\">Check me out</b-form-checkbox>\n                <b-form-checkbox value=\"that\">Check that out</b-form-checkbox>\n              </b-form-checkbox-group>\n            </b-form-group>\n\n            <b-button type=\"submit\" variant=\"primary\">Submit</b-button>\n            <b-button type=\"reset\" variant=\"danger\">Reset</b-button>\n          </b-form>\n          <b-card class=\"mt-3\" header=\"Form Data Result\">\n            <pre class=\"m-0\">{{ form }}</pre>\n          </b-card>\n        </div>\n      </div>\n    </div>\n  </main>\n</template>\n\n<script>\nexport default {\n  name: \"InputGuest\",\n  data() {\n    return {\n      form: {\n        email: \"\",\n        name: \"\",\n        food: null,\n        checked: [],\n      },\n      foods: [\n        { text: \"Select One\", value: null },\n        \"Carrots\",\n        \"Beans\",\n        \"Tomatoes\",\n        \"Corn\",\n      ],\n      show: true,\n    };\n  },\n  methods: {\n    onSubmit(event) {\n      event.preventDefault();\n      alert(JSON.stringify(this.form));\n    },\n    onReset(event) {\n      event.preventDefault();\n      // Reset our form values\n      this.form.email = \"\";\n      this.form.name = \"\";\n      this.form.food = null;\n      this.form.checked = [];\n      // Trick to reset/clear native browser form validation state\n      this.show = false;\n      this.$nextTick(() => {\n        this.show = true;\n      });\n    },\n  },\n};\n</script>\n\n<style></style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}