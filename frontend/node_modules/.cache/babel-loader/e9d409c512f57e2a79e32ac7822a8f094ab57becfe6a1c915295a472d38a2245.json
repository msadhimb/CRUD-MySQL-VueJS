{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"GuestList\",\n  data() {\n    return {\n      guests: [],\n      form: {\n        nama: \"\",\n        email: \"\"\n      }\n    };\n  },\n  mounted() {\n    this.getAllGuest();\n  },\n  methods: {\n    getAllGuest() {\n      axios.get(\"http://localhost:4000/tamu\").then(response => {\n        this.guests = response.data;\n      });\n    },\n    deleteGuest(id) {\n      axios.delete(\"http://localhost:4000/tamu/\" + id).then(response => {\n        console.log(response);\n        this.getAllGuest();\n      });\n    },\n    handleUpdate(id) {\n      axios.get(\"http://localhost:4000/tamu/\" + id).then(response => {\n        const data = response.data;\n        this.form.nama = data.nama;\n        this.form.email = data.email;\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAkGA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAI,GAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,IAAI,EAAE;QACJC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE;MACT;IACF,CAAC;EACH,CAAC;EACDC,OAAO,GAAG;IACR,IAAI,CAACC,WAAW,EAAE;EACpB,CAAC;EACDC,OAAO,EAAE;IACPD,WAAW,GAAG;MACZR,KAAK,CAACU,GAAG,CAAC,4BAA4B,CAAC,CAACC,IAAI,CAAEC,QAAQ,IAAK;QACzD,IAAI,CAACT,MAAK,GAAIS,QAAQ,CAACV,IAAI;MAC7B,CAAC,CAAC;IACJ,CAAC;IACDW,WAAW,CAACC,EAAE,EAAE;MACdd,KAAK,CAACe,MAAM,CAAC,6BAA4B,GAAID,EAAE,CAAC,CAACH,IAAI,CAAEC,QAAQ,IAAK;QAClEI,OAAO,CAACC,GAAG,CAACL,QAAQ,CAAC;QACrB,IAAI,CAACJ,WAAW,EAAE;MACpB,CAAC,CAAC;IACJ,CAAC;IACDU,YAAY,CAACJ,EAAE,EAAE;MACfd,KAAK,CAACU,GAAG,CAAC,6BAA4B,GAAII,EAAE,CAAC,CAACH,IAAI,CAAEC,QAAQ,IAAK;QAC/D,MAAMV,IAAG,GAAIU,QAAQ,CAACV,IAAI;QAC1B,IAAI,CAACE,IAAI,CAACC,IAAG,GAAIH,IAAI,CAACG,IAAI;QAC1B,IAAI,CAACD,IAAI,CAACE,KAAI,GAAIJ,IAAI,CAACI,KAAK;MAC9B,CAAC,CAAC;IACJ;EACF;AACF,CAAC","names":["axios","name","data","guests","form","nama","email","mounted","getAllGuest","methods","get","then","response","deleteGuest","id","delete","console","log","handleUpdate"],"sourceRoot":"","sources":["/Users/adhim/Documents/Working/VueJs/crudmysql/frontend/src/components/List.vue"],"sourcesContent":["<template>\n  <main>\n    <h1>Guest List</h1>\n    <div class=\"container\">\n      <table class=\"table table-striped\">\n        <thead>\n          <tr>\n            <th scope=\"col\">#</th>\n            <th scope=\"col\">Name</th>\n            <th scope=\"col\">Email Address</th>\n            <th scope=\"col\">Action</th>\n          </tr>\n        </thead>\n        <tbody>\n          <tr v-for=\"guest in guests\" v-bind:key=\"guest.id\">\n            <th scope=\"row\">1</th>\n            <td>{{ guest.nama }}</td>\n            <td>{{ guest.email == null ? \"Kosong\" : guest.email }}</td>\n            <td>\n              <button\n                class=\"btn btn-primary me-2\"\n                data-bs-toggle=\"modal\"\n                data-bs-target=\"#exampleModal\"\n                @click=\"handleUpdate(guest.id)\"\n              >\n                Edit\n              </button>\n              <button class=\"btn btn-danger\" @click=\"deleteGuest(guest.id)\">\n                Delete\n              </button>\n            </td>\n          </tr>\n        </tbody>\n      </table>\n    </div>\n\n    <!-- Modal -->\n    <div\n      class=\"modal fade\"\n      id=\"exampleModal\"\n      tabindex=\"-1\"\n      aria-labelledby=\"exampleModalLabel\"\n      aria-hidden=\"true\"\n    >\n      <div class=\"modal-dialog modal-dialog-centered\">\n        <div class=\"modal-content\">\n          <div class=\"modal-header\">\n            <h1 class=\"modal-title fs-5\" id=\"exampleModalLabel\">Edit Data</h1>\n            <button\n              type=\"button\"\n              class=\"btn-close\"\n              data-bs-dismiss=\"modal\"\n              aria-label=\"Close\"\n            ></button>\n          </div>\n          <form>\n            <div class=\"modal-body\">\n              <div class=\"form-floating mb-3\">\n                <input\n                  type=\"email\"\n                  class=\"form-control\"\n                  id=\"floatingInput\"\n                  placeholder=\"name@example.com\"\n                  v-model=\"form.email\"\n                />\n                <label for=\"floatingInput\">Email address</label>\n              </div>\n              <div class=\"form-floating\">\n                <input\n                  type=\"text\"\n                  class=\"form-control\"\n                  id=\"floatingName\"\n                  placeholder=\"Name\"\n                  v-model=\"form.nama\"\n                />\n                <label for=\"floatingName\">Name</label>\n              </div>\n            </div>\n            <div class=\"modal-footer\">\n              <button\n                type=\"button\"\n                class=\"btn btn-secondary\"\n                data-bs-dismiss=\"modal\"\n              >\n                Close\n              </button>\n              <button type=\"button\" class=\"btn btn-primary\">\n                Save changes\n              </button>\n            </div>\n          </form>\n        </div>\n      </div>\n    </div>\n  </main>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"GuestList\",\n  data() {\n    return {\n      guests: [],\n      form: {\n        nama: \"\",\n        email: \"\",\n      },\n    };\n  },\n  mounted() {\n    this.getAllGuest();\n  },\n  methods: {\n    getAllGuest() {\n      axios.get(\"http://localhost:4000/tamu\").then((response) => {\n        this.guests = response.data;\n      });\n    },\n    deleteGuest(id) {\n      axios.delete(\"http://localhost:4000/tamu/\" + id).then((response) => {\n        console.log(response);\n        this.getAllGuest();\n      });\n    },\n    handleUpdate(id) {\n      axios.get(\"http://localhost:4000/tamu/\" + id).then((response) => {\n        const data = response.data;\n        this.form.nama = data.nama;\n        this.form.email = data.email;\n      });\n    },\n  },\n};\n</script>\n\n<style></style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}