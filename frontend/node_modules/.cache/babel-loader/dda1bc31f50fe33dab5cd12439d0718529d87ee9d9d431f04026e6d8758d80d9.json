{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"GuestList\",\n  data() {\n    return {\n      guests: []\n    };\n  },\n  mounted() {\n    axios.get(\"http://localhost:4000/tamu\").then(response => {\n      this.guests = response.data;\n    });\n  },\n  methods: {\n    deleteGuest(id) {\n      axios.delete(\"http://localhost:4000/tamu/\" + id).then(response => {\n        console.log(response);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAgCA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAI,GAAG;IACL,OAAO;MACLC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,OAAO,GAAG;IACRJ,KAAK,CAACK,GAAG,CAAC,4BAA4B,CAAC,CAACC,IAAI,CAAEC,QAAQ,IAAK;MACzD,IAAI,CAACJ,MAAK,GAAII,QAAQ,CAACL,IAAI;IAC7B,CAAC,CAAC;EACJ,CAAC;EACDM,OAAO,EAAE;IACPC,WAAW,CAACC,EAAE,EAAE;MACdV,KAAK,CAACW,MAAM,CAAC,6BAA4B,GAAID,EAAE,CAAC,CAACJ,IAAI,CAAEC,QAAQ,IAAK;QAClEK,OAAO,CAACC,GAAG,CAACN,QAAQ,CAAC;MACvB,CAAC,CAAC;IACJ;EACF;AACF,CAAC","names":["axios","name","data","guests","mounted","get","then","response","methods","deleteGuest","id","delete","console","log"],"sourceRoot":"","sources":["/Users/adhim/Documents/Working/VueJs/crudmysql/frontend/src/components/List.vue"],"sourcesContent":["<template>\n  <main>\n    <h1>Guest List</h1>\n    <div class=\"container\">\n      <table class=\"table table-striped\">\n        <thead>\n          <tr>\n            <th scope=\"col\">#</th>\n            <th scope=\"col\">Name</th>\n            <th scope=\"col\">Email Address</th>\n            <th scope=\"col\">Action</th>\n          </tr>\n        </thead>\n        <tbody>\n          <tr v-for=\"guest in guests\" v-bind:key=\"guest.id\">\n            <th scope=\"row\">1</th>\n            <td>{{ guest.nama }}</td>\n            <td>{{ guest.email == null ? \"Kosong\" : guest.email }}</td>\n            <td>\n              <button class=\"btn btn-primary me-2\">Edit</button>\n              <button class=\"btn btn-danger\" @click=\"deleteGuest(guest.id)\">\n                Delete\n              </button>\n            </td>\n          </tr>\n        </tbody>\n      </table>\n    </div>\n  </main>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"GuestList\",\n  data() {\n    return {\n      guests: [],\n    };\n  },\n  mounted() {\n    axios.get(\"http://localhost:4000/tamu\").then((response) => {\n      this.guests = response.data;\n    });\n  },\n  methods: {\n    deleteGuest(id) {\n      axios.delete(\"http://localhost:4000/tamu/\" + id).then((response) => {\n        console.log(response);\n      });\n    },\n  },\n};\n</script>\n\n<style></style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}