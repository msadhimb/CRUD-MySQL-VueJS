{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"InputGuest\",\n  data() {\n    return {\n      forms: {\n        email: \"\",\n        nama: \"\"\n      }\n    };\n  },\n  methods: {\n    addGuest() {\n      axios.post(\"http://localhost:4000/tamu\", this.forms).then(res => {\n        console.log(res);\n      }).catch(err => {\n        console.log(err);\n      });\n      this.forms.email = \"\";\n      this.forms.nama = \"\";\n    }\n  }\n};","map":{"version":3,"mappings":"AAkDA,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE;QACLC,KAAK,EAAE,EAAE;QACTC,IAAI,EAAE;MACR;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQ,GAAG;MACTP,KAAI,CACDQ,IAAI,CAAC,4BAA4B,EAAE,IAAI,CAACL,KAAK,EAC7CM,IAAI,CAAEC,GAAG,IAAK;QACbC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;MAClB,CAAC,EACAG,KAAK,CAAEC,GAAG,IAAK;QACdH,OAAO,CAACC,GAAG,CAACE,GAAG,CAAC;MAClB,CAAC,CAAC;MACJ,IAAI,CAACX,KAAK,CAACC,KAAI,GAAI,EAAE;MACrB,IAAI,CAACD,KAAK,CAACE,IAAG,GAAI,EAAE;IACtB;EACF;AACF,CAAC","names":["axios","name","data","forms","email","nama","methods","addGuest","post","then","res","console","log","catch","err"],"sourceRoot":"","sources":["/Users/adhim/Documents/Working/VueJs/crudmysql/frontend/src/components/Input.vue"],"sourcesContent":["<template>\n  <main>\n    <div class=\"container pt-5\">\n      <h1 class=\"w-100\">Congratulation on your wedding</h1>\n      <div class=\"row d-flex align-items-center\">\n        <div class=\"col-md-6\">\n          <img\n            src=\"../assets/undraw_wedding_re_66hj.png\"\n            alt=\"wedding\"\n            class=\"d-block img-fluid\"\n          />\n        </div>\n        <div class=\"col-md-6\">\n          <form @submit.prevent=\"addGuest()\">\n            <div class=\"form-floating mb-3\">\n              <input\n                type=\"email\"\n                class=\"form-control\"\n                id=\"floatingInput\"\n                placeholder=\"name@example.com\"\n                :value=\"this.forms.email\"\n                @input=\"(event) => (this.forms.email = event.target.value)\"\n              />\n              <label for=\"floatingInput\">Email address</label>\n            </div>\n            <div class=\"form-floating\">\n              <input\n                type=\"text\"\n                class=\"form-control\"\n                id=\"floatingName\"\n                placeholder=\"Name\"\n                :value=\"this.forms.nama\"\n                @input=\"(event) => (this.forms.nama = event.target.value)\"\n              />\n              <label for=\"floatingName\">Name</label>\n            </div>\n            <button type=\"submit\" class=\"btn btn-primary mt-3\">Submit</button>\n          </form>\n        </div>\n      </div>\n      <div class=\"div d-flex justify-content-end\">\n        <router-link to=\"/guest\" class=\"btn btn-success\"\n          >See Guest List</router-link\n        >\n      </div>\n    </div>\n  </main>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n  name: \"InputGuest\",\n  data() {\n    return {\n      forms: {\n        email: \"\",\n        nama: \"\",\n      },\n    };\n  },\n  methods: {\n    addGuest() {\n      axios\n        .post(\"http://localhost:4000/tamu\", this.forms)\n        .then((res) => {\n          console.log(res);\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n      this.forms.email = \"\";\n      this.forms.nama = \"\";\n    },\n  },\n};\n</script>\n\n<style></style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}